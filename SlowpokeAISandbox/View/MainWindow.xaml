<Window x:Class="SlowpokeAISandbox.View.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:local="clr-namespace:SlowpokeAISandbox.ViewModel"
        Title="MainWindow" Height="800" Width="1200">
      
   
    <Window.DataContext>
        <local:MainWindowViewModel></local:MainWindowViewModel>
    </Window.DataContext>
    <Grid>
        <Canvas Height="800" Width="1000" Background="DarkGreen">

            <Ellipse
          Canvas.Top="50"
          Canvas.Left="50"
          Fill="#FFFFFF00"
          Height="75"
          Width="75"
          StrokeThickness="5"
          Stroke="#FF0000FF"/>
        
        </Canvas>
    </Grid>

</Window>
